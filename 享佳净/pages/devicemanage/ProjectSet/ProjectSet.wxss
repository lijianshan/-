
.passwordInputView{
  width: 100%;
  height: 100rpx;
  margin-top: 200rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.passwordText {
  border: 1rpx solid #d5d5d5;
  border-radius: 5px;
  color: #545454;
  text-align: center;
  background-color: white;
}
.titlesView {
  width: 100%;
  height: 10%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.titles {
  width: 50%;
  height: 90%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.on {
  border-bottom: 10rpx solid #eea231;
}

.cell {
  width: 100%;
  height: 100rpx;
  font-size: 16px;
  border-bottom: 2rpx solid #f5f5f5;
  background-color: white;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.line{
  height: 50rpx;
  background-color: #f5f5f5;
}
.title {
  width: 13%;
  margin-left: 30rpx;
}
input{
  width: 80%;
  height: 100%;
}
.buttonView{
  margin-top: 20rpx;
  width: 100%;
  height: 100rpx;
  font-size: 16px;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

button {
  width: 30%;
  height: 100%;
  justify-content: center;
  color: white;
  background-color: #068de7;
}